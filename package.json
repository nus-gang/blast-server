{
  "name": "blast-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/Main.ts",
  "scripts": {
    "start:test": "ts-node -r tsconfig-paths/register test.ts",
    "start:local": "listen_port=4000 stage=local ts-node -r tsconfig-paths/register src/Main.ts",
    "start:prod": "listen_port=80 stage=prod ts-node -r tsconfig-paths/register src/Main.ts",
    "start:main": "ts-node -r tsconfig-paths/register src/Main.ts",
    "build": "nest build",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/scripts/DataSource.ts",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- --transaction all migration:run",
    "migration:run_fake": "npm run typeorm -- --transaction all migration:run --fake",
    "migration:revert": "npm run typeorm -- --transaction all migration:revert",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "license": "MIT",
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/typeorm": "^10.0.0",
    "@types/app-root-path": "^1.2.6",
    "@types/body-parser": "^1.19.3",
    "@types/deep-equal": "^1.0.2",
    "@types/http-proxy": "^1.17.12",
    "@types/js-yaml": "^4.0.6",
    "@types/node": "^20.7.1",
    "@types/object-hash": "^3.0.4",
    "app-root-path": "^3.1.0",
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.2.3",
    "deep-equal": "^2.2.2",
    "js-yaml": "^4.1.0",
    "mysql2": "^3.6.1",
    "nest-winston": "^1.9.4",
    "object-hash": "^3.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "ts-stopwatch": "^0.0.4",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "web3": "^4.2.2",
    "web3-eth-contract": "^4.1.3",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "onchange": "^7.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint",
      "pre-push": ""
    }
  }
}
